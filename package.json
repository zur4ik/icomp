{
  "name": "icomp",
  "description": "A CLI tool for generating React components from SVG icons.",
  "private": false,
  "version": "1.1.7",
  "author": "Zura Jijavadze",
  "license": "MIT",
  "type": "module",
  "bin": {
    "icomp": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zur4ik/icomp.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "svg",
    "icons",
    "react-components",
    "component-generator",
    "svg-to-react",
    "cli-tool",
    "open-source"
  ],
  "scripts": {
    "exp:dev": "vite --mode explorer",
    "exp:build": "vite build --mode explorer",
    "exp:preview": "vite preview --mode explorer",
    "cli:build": "vite build --mode cli && chmod +x dist/cli.js",
    "build": "npm run cli:build && npm run exp:build && chmod +x dist/cli.js",
    "pack:local": "npm run build && mkdir -p out && npm pack --pack-destination ./out",
    "lint": "eslint .",
    "format": "prettier --write .",
    "version": "standard-version",
    "pub:check": "npm run build && npm publish --dry-run",
    "publish": "git push --follow-tags origin main && npm publish"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "express": "^4.21.2",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "standard-version": "^9.5.0",
    "svgo": "^3.3.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
