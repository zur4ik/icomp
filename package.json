{
  "name": "icomp",
  "description": "A CLI tool for generating React components from SVG icons.",
  "private": false,
  "version": "1.1.7",
  "author": "Zura Jijavadze",
  "license": "MIT",
  "type": "module",
  "bin": {
    "icomp": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zur4ik/icomp.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "svg",
    "icons",
    "react-components",
    "component-generator",
    "svg-to-react",
    "cli-tool",
    "open-source"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "cli:dev": "tsx ./bin/cli.ts",
    "cli:build": "tsc --project tsconfig.cli.json"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.7.0",
    "typescript": "^5.2.0",
    "vite": "^6.0.0"
  }
}
