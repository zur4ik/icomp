{
  "name": "icomp",
  "description": "A CLI tool for generating React components from SVG icons.",
  "private": false,
  "version": "1.1.7",
  "author": "Zura Jijavadze",
  "license": "MIT",
  "type": "module",
  "bin": {
    "icomp": "dist/cli/cli.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zur4ik/icomp.git"
  },
  "files": [
    "dist",
    "README.md",
    "!dist/cli/.cache"
  ],
  "keywords": [
    "react",
    "svg",
    "icons",
    "react-components",
    "component-generator",
    "svg-to-react",
    "cli-tool",
    "open-source"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "cli:dev": "tsx --tsconfig ./tsconfig.cli.json ./cli/index.ts generate -i .assets -o src/com/icons",
    "cli:build": "rollup -c && chmod +x ./dist/cli/cli.cjs",
    "format": "prettier --write .",
    "lint": "eslint .",
    "pack:local": "npm run build && npm run cli:build && mkdir -p out && npm pack --pack-destination ./out",
    "test:pack": "npm run build && npm run cli:build && npm publish --dry-run",
    "icomp:link": "npm run cli:build && npm link",
    "icomp:test": "icomp generate -i ./.assets -o ./src/com/icons"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "esbuild": "^0.25.2",
    "express": "^4.18.2",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "rollup": "^4.39.0",
    "rollup-plugin-delete": "^3.0.1",
    "svgo": "^3.3.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.0",
    "vite": "^6.0.0"
  }
}
